extends topmenu/potensigangguanhome.pug
block content-header
    .panel-header.panel-header-sm
block content
    .row
      .col-md-8
        .card
          .card-header
            h5.title Potensi Gangguan (IKU1)
          .card-body
            form(action='/potensigangguan/'+ edit method='POST')
              .row
                .col-md-6.pr-1
                  .form-group
                    label Tahun
                    input.form-control(type='text' name="tahun_potensi_gangguan" placeholder='Masukkan Tahun' value='2020')
              .row
                .col-md-6.pr-1
                  .form-group
                    label Jumlah
                    input.form-control(type='text' name="jumlah_potensi_gangguan" placeholder='Masukkan Jumlah Potensi' value='')
              .row
                .col-md-12
                  .form-group
                    label Upload Dokumen
                    input.btn.btn-info.btn-round(type='file' name="attachment_potensi_gangguan" placeholder='Dokumen' value='Potensi.doc') 
              .row
                .col-md-6.pr-1
                  .form-group
                    input.btn.btn-info.btn-round(type='submit' placeholder='' value='Submit')    